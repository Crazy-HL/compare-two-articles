== \"div1\";\n      buttonStyle.value = {\n        position: \"absolute\",\n        top: \"10px\",\n        [isDiv1 ? \"right\" : \"left\"]: \"10px\"\n      };\n      outlineStyle.value = {\n        position: \"absolute\",\n        top: \"50px\",\n        [isDiv1 ? \"right\" : \"left\"]: \"10px\",\n        width: \"260px\",\n        maxHeight: \"80vh\",\n        overflowY: \"auto\",\n        zIndex: \"1000\",\n        background: \"white\",\n        borderRadius: \"8px\",\n        boxShadow: \"0 4px 12px rgba(0, 0, 0, 0.15)\",\n        padding: \"15px\"\n      };\n    };\n\n    // 滚动事件处理，用于同步两篇文章的滚动\n    const handleScroll = () => {\n      if (!scrollSyncEnabled.value) return; // 如果不同步滚动，则直接返回\n\n      const referenceElement = document.getElementById(props.divId);\n      if (!referenceElement) return;\n      const scrollY = referenceElement.scrollTop;\n      // 更新按钮和大纲的位置\n      buttonStyle.value.top = `${scrollY + 10}px`;\n      outlineStyle.value.top = `${scrollY + 50}px`;\n\n      // 找到当前文章大纲中与滚动位置相匹配的章节\n      const matchedChapter = outline.value.find((item, index) => {\n        const element = document.getElementById(item.id);\n        if (element) {\n          const rect = element.getBoundingClientRect();\n          return rect.top <= window.innerHeight && rect.bottom >= 0;\n        }\n        return false;\n      });\n      if (matchedChapter) {\n        // 同步另一篇文章滚动到对应章节\n        const linkedChapter = props.linkedOutline.find(linked => linked.leftId === matchedChapter.id || linked.rightId === matchedChapter.id);\n        if (linkedChapter) {\n          const linkedElement = document.getElementById(linkedChapter.rightId);\n          if (linkedElement) {\n            scrollSyncEnabled.value = false; // 禁止同步滚动\n            linkedElement.scrollIntoView({\n              behavior: \"smooth\"\n            });\n            scrollSyncEnabled.value = true; // 重新启用同步滚动\n          }\n        }\n      }\n    };\n\n    // 初始化时添加滚动监听\n    onMounted(() => {\n      outline.v