d_2","_hoisted_3","_cache","$event","$setup","leftArticleUrl","placeholder","onKeyup","_withKeys","loadLeftArticle","onClick","compareArticles","disabled","isComparing","_hoisted_4","_hoisted_5","rightArticleUrl","loadRightArticle","onDragover","_withModifiers","onDrop","handleDrop","_hoisted_6","_Fragment","_renderList","chatHistory","message","index","_normalizeClass","role","_hoisted_7","_toDisplayString","innerHTML","content","_hoisted_8","comparisonResult","_hoisted_9","structure","section","title","type","_hoisted_10","items","item","name","_hoisted_11","_hoisted_12","value","_hoisted_13","_hoisted_14","field","same","is_same","different","difference","_hoisted_15","_hoisted_16","_hoisted_17","_createVNode","div1RawData","div3RawData","onCompareAttribute","handleAttributeComparison","_hoisted_18","_hoisted_19","userQuestion","rows","askQuestion","mergedJson","compareTexts"],"sources":["D:\\vue_frame\\vue_frame\\wikitable-vue\\client\\src\\components\\Div2.vue"],"sourcesContent":["<template>\r\n\t<div class=\"main-container\">\r\n\t\t<!-- 新增对比控制面板 -->\r\n\t\t<div class=\"comparison-controls\">\r\n\t\t\t<div class=\"url-input\">\r\n\t\t\t\t<input\r\n\t\t\t\t\tv-model=\"leftArticleUrl\"\r\n\t\t\t\t\tplaceholder=\"左侧文章URL (如: Python_(programming_language)\"\r\n\t\t\t\t\t@keyup.enter=\"loadLeftArticle\" />\r\n\t\t\t\t<button @click=\"loadLeftArticle\">加载</button>\r\n\t\t\t</div>\r\n\r\n\t\t\t<button\r\n\t\t\t\tclass=\"compare-btn\"\r\n\t\t\t\t@click=\"compareArticles\"\r\n\t\t\t\t:disabled=\"isComparing\">\r\n\t\t\t\t{{ isComparing ? \"对比中...\" : \"对比文章\" }}\r\n\t\t\t</button>\r\n\r\n\t\t\t<div class=\"url-input\">\r\n\t\t\t\t<input\r\n\t\t\t\t\tv-model=\"rightArticleUrl\"\r\n\t\t\t\t\tplaceholder=\"右侧文章URL (如: Python)\"\r\n\t\t\t\t\t@keyup.enter=\"loadRightArticle\" />\r\n\t\t\t\t<button @click=\"loadRightArticle\">加载</button>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\t\t<!-- 原有聊天容器 -->\r\n\t\t<div class=\"chat-container\" @dragover.prevent @drop=\"handleDrop\">\r\n\t\t\t<div class=\"chat-history\">\r\n\t\t\t\t<div\r\n\t\t\t\t\tv-for=\"(message, index) in chatHistory\"\r\n\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t:class=\"['message', message.role]\">\r\n\t\t\t\t\t<div class=\"message-content\">\r\n\t\t\t\t\t\t<strong>{{ message.role === \"user\" ? \"用户\" : \"GPT\" }}:</strong>\r\n\t\t\t\t\t\t<p v-html=\"message.content\"></p>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\t\t<!-- 对比结果可视化区域 -->\r\n\t\t<div class=\"comparison-vis-container\" v-if=\"comparisonResult\">\r\n\t\t\t<div\r\n\t\t\t\tclass=\"comparison-section\"\r\n\t\t\t\tv-for=\"(section, index) in comparisonResult.structure\"\r\n\t\t\t\t:key=\"index\">\r\n\t\t\t\t<h3>{{ section.title }}</h3>\r\n\r\n\t\t\t\t<!-- 元数据对比 -->\r\n\t\t\t\t<div v-if=\"section.type === 'metadata'\" class=\"metadata-comparison\">\r\n\t\t\t\t\t<div\r\n\t\t\t\t\t\tv-for=\"item in section.items\"\r\n\t\t\t\t\t\t:key=\"item.name\"\r\n\t\t\t\t\t\tclass=\"metadata-item\">\r\n\t\t\t\t\t\t<span class=\"item-name\">{{ item.name }}:</span>\r\n\t\t\t\t\t\t<span class=\"item-value\">{{ item.value }}</span>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\r\n\t\t\t\t<!-- Infobox对比 -->\r\n\t\t\t\t<div v-if=\"section.type === 'infobox'\" class=\"infobox-comparison\">\r\n\t\t\t\t\t<table>\r\n\t\t\t\t\t\t<thead>\r\n\t\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t\t<th>属性</th>\r\n\t\t\t\t\t\t\t\t<th>状态</th>\r\n\t\t\t\t\t\t\t\t<th>差异描述</th>\r\n\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t</thead>\r\n\t\t\t\t\t\t<tbody>\r\n\t\t\t\t\t\t\t<tr v-for=\"item in section.items\" :key=\"item.field\">\r\n\t\t\t\t\t\t\t\t<td>{{ item.field }}</td>\r\n\t\t\t\t\t\t\t\t<td :class=\"{ same: item.is_same, different: !item.is_same }\">\r\n\t\t\t\t\t\t\t\t\t{{ item.is_same ? \"相同\" : \"不同\" }}\r\n\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t<td>{{ item.difference }}</td>\r\n\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t</tbody>\r\n\t\t\t\t\t</table>\r\n\t\t\t\t</div>\r\n\r\n\t\t\t\t<!-- 章节对比 -->\r\n\t\t\t\t<div\r\n\t\t\t\t\tv-if=\"section.type === 'section'\"\r\n\t\t\t\t\tclass=\"section-comparison\"\r\n\t\t\t\t\tv-html=\"section.content\"></div>\r\n\r\n\t\t\t\t<!-- 总结 -->\r\n\t\t\t\t<div\r\n\t\t\t\t\tv-if=\"section.type === 'summary'\"\r\n\t\t\t\t\tclass=\"summary-comparison\"\r\n\t\t\t\t\tv-html=\"section.content\"></div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\t\t<!-- 原有可视化容器 -->\r\n\t\t<div class=\"vis-container\">\r\n\t\t\t<CompareTable\r\n\t\t\t\tclass=\"compare-table\"\r\n\t\t\t\t:div1-raw-data=\"div1RawData\"\r\n\t\t\t\t:div3-raw-data=\"div3RawData\"\r\n\t\t\t\t@compareAttribute=\"handleAttributeComparison\" />\r\n\t\t</div>\r\n\r\n\t\t<!-- 原有输入区域 -->\r\n\t\t<div class=\"input\">\r\n\t\t\t<div class=\"input-container\">\r\n\t\t\t\t<textarea\r\n\t\t\t\t\tv-model=\"userQuestion\"\r\n\t\t\t\t\trows=\"2\"\r\n\t\t\t\t\tplaceholder=\"请输入你想问的问题...\"></textarea>\r\n\t\t\t\t<div class=\"button-container\">\r\n\t\t\t\t\t<button @click=\"askQuestion\">发送</button>\r\n\t\t\t\t\t<button @click=\"mergedJson\">合并数据</button>\r\n\t\t\t\t\t<button @click=\"compareTexts\">对比文本</button>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script setup>\r\n\timport { ref, onMounted, onUnmounted } from \"vue\";\r\n\timport bus from \"@/js/eventBus.js\";\r\n\timport CompareTable from \"@/components/compoents_base/CompareTable.vue\";\r\n\timport {\r\n\t\trenderLineChart,\r\n\t\trenderBarChart,\r\n\t\trenderPieChart,\r\n\t\trenderNonVisualChart,\r\n\t\trenderRadarChart,\r\n\t\trenderStackedBarChart,\r\n\t\trenderScatterChart\r\n\t} from \"@/js/chartUtils\";\r\n\r\n\t// 原有状态\r\n\tconst userQuestion = ref(\"\");\r\n\tconst chatHistory = ref([]);\r\n\tconst selectText2 = ref(\"\");\r\n\tconst selectText3 = ref(\"\");\r\n\tconst currentChartData = ref(null);\r\n\tconst currentChartType = ref(null);\r\n\tconst div1RawData = ref(null);\r\n\tconst div3RawData = ref(null);\r\n\r\n\t// 新增对比功能状态\r\n\tconst leftArticleUrl = ref(\"\");\r\n\tconst rightArticleUrl = ref(\"\");\r\n\tconst comparisonResult = ref(null);\r\n\tconst isComparing = ref(false);\r\n\r\n\t// 原有事件处理\r\n\tconst handleDiv1Event = data => handleSelection(data, \"div1\");\r\n\tconst handleDiv3Event = data => handleSelection(data, \"div3\");\r\n\r\n\tonMounted(() => {\r\n\t\tbus.on(\"div1_Event\", handleDiv1Event);\r\n\t\tbus.on(\"div3_Event\", handleDiv3Event);\r\n\t});\r\n\r\n\tonUnmounted(() => {\r\n\t\tbus.off(\"div1_Event\", handleDiv1Event);\r\n\t\tbus.off(\"div3_Event\", handleDiv3Event);\r\n\t});\r\n\r\n\t// 新增方法 - 加载文章\r\n\tconst loadLeftArticle = async () => {\r\n\t\tif (!leftArticleUrl.value) return;\r\n\t\ttry {\r\n\t\t\tconst response = await api.post(\"fetch_wikipedia\", {\r\n\t\t\t\turl: leftArticleUrl.value\r\n\t\t\t});\r\n\t\t\tdiv1RawData.value = response.content;\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: `已加载左侧文章: <strong>${leftArticleUrl.value}</strong>`\r\n\t\t\t});\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"加载文章失败:\", error);\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: \"加载左侧文章失败，请检查URL是否正确\"\r\n\t\t\t});\r\n\t\t}\r\n\t};\r\n\r\n\tconst loadRightArticle = async () => {\r\n\t\tif (!rightArticleUrl.value) return;\r\n\t\ttry {\r\n\t\t\tconst response = await api.post(\"fetch_wikipedia\", {\r\n\t\t\t\turl: rightArticleUrl.value\r\n\t\t\t});\r\n\t\t\tdiv3RawData.value = response.content;\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: `已加载右侧文章: <strong>${rightArticleUrl.value}</strong>`\r\n\t\t\t});\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"加载文章失败:\", error);\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: \"加载右侧文章失败，请检查URL是否正确\"\r\n\t\t\t});\r\n\t\t}\r\n\t};\r\n\r\n\t// 新增方法 - 对比两篇文章\r\n\tconst compareArticles = async () => {\r\n\t\tif (!leftArticleUrl.value || !rightArticleUrl.value) {\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: \"请先加载两篇文章\"\r\n\t\t\t});\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\tisComparing.value = true;\r\n\t\tchatHistory.value.push({\r\n\t\t\trole: \"assistant\",\r\n\t\t\tcontent: \"正在对比两篇文章...\"\r\n\t\t});\r\n\r\n\t\ttry {\r\n\t\t\tconst response = await api.post(\"compare_articles\", {\r\n\t\t\t\tarticle1_url: leftArticleUrl.value,\r\n\t\t\t\tarticle2_url: rightArticleUrl.value\r\n\t\t\t});\r\n\r\n\t\t\tif (response.success) {\r\n\t\t\t\tcomparisonResult.value = response.result;\r\n\t\t\t\tchatHistory.value.push({\r\n\t\t\t\t\trole: \"assistant\",\r\n\t\t\t\t\tcontent: `对比完成！发现 ${response.result.structure.length} 个主要对比方面。<br>\r\n                  主要差异: ${response.result.summary}`\r\n\t\t\t\t});\r\n\t\t\t} else {\r\n\t\t\t\tthrow new Error(response.error || \"对比失败\");\r\n\t\t\t}\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"对比失败:\", error);\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: `对比失败: ${error.message}`\r\n\t\t\t});\r\n\t\t} finally {\r\n\t\t\tisComparing.value = false;\r\n\t\t}\r\n\t};\r\n\r\n\t// 保留原有方法\r\n\tfunction handleSelection(data, source) {\r\n\t\tconst plainText = getPlainTextFromSelection(data.content);\r\n\t\tif (source === \"div1\") {\r\n\t\t\tselectText2.value = plainText;\r\n\t\t\tdiv1RawData.value = data.content;\r\n\t\t} else if (source === \"div3\") {\r\n\t\t\tselectText3.value = plainText;\r\n\t\t\tdiv3RawData.value = data.content;\r\n\t\t}\r\n\t}\r\n\r\n\tfunction getPlainTextFromSelection(htmlContent) {\r\n\t\tconst container = document.createElement(\"div\");\r\n\t\tcontainer.innerHTML = htmlContent;\r\n\t\treturn container.innerText || container.textContent || \"\";\r\n\t}\r\n\r\n\tconst handleDragStart = event => {\r\n\t\tevent.dataTransfer.setData(\r\n\t\t\t\"application/json\",\r\n\t\t\tJSON.stringify({\r\n\t\t\t\tchartData: currentChartData.value,\r\n\t\t\t\tchartType: currentChartType.value\r\n\t\t\t})\r\n\t\t);\r\n\t};\r\n\r\n\tconst handleDrop = event => {\r\n\t\tevent.preventDefault();\r\n\t\tconst data = event.dataTransfer.getData(\"applic