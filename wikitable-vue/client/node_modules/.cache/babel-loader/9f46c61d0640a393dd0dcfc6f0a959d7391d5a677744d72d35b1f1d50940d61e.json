{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, onUnmounted, nextTick, watch } from \"vue\";\nimport LoadingSpinner from \"./LoadingSpinner.vue\";\nimport ErrorDisplay from \"./ErrorDisplay.vue\";\nimport WikipediaContent from \"./WikipediaContent.vue\";\nimport TextPopup from \"./TextPopup.vue\";\nimport ArticleOutline from \"./ArticleOutline.vue\";\nimport * as d3 from \"d3\";\nimport bus from \"@/js/eventBus.js\";\nimport { parseInfoboxForComparison } from \"@/js/wikiInfoboxParser\";\nexport default {\n  components: {\n    LoadingSpinner,\n    ErrorDisplay,\n    WikipediaContent,\n    TextPopup,\n    ArticleOutline\n  },\n  props: {\n    pageTitle: String,\n    divId: String,\n    selectContentClass: String,\n    linkedOutline: Array\n  },\n  setup(props) {\n    const pageHtml = ref(\"\");\n    const loading = ref(true);\n    const error = ref(\"\");\n    const selectedText = ref({});\n    const showPopup = ref({});\n    const divRef = ref(null);\n    const MainTextContent = ref(\"\");\n    const highlightMap = ref({}); // 存储高亮文本 {divId: [texts]}\n    const highlightedElements = ref({}); // 存储高亮DOM元素\n\n    // 原有fetchWikipediaContent函数保持不变\n    const fetchWikipediaContent = async () => {\n      try {\n        const apiUrl = `https://en.wikipedia.org/api/rest_v1/page/html/${encodeURIComponent(props.pageTitle)}`;\n        const response = await fetch(apiUrl);\n        if (!response.ok) throw new Error(\"Failed to fetch data\");\n        const html = await response.text();\n        MainTextContent.value = cleanWikipediaContent(html);\n        const parser = new DOMParser();\n        const doc = parser.parseFromString(html, \"text/html\");\n        const infobox = doc.querySelector(\".infobox, .sidebar, .toccolours\");\n        const comparisonData = parseInfoboxForComparison(infobox);\n        const sidebar = doc.querySelector(\".sidebar\");\n        if (sidebar) sidebar.remove();\n        bus.emit(`${props.divId}_InfoboxData`, comparisonData);\n        const headings = doc.querySelectorAll(\"h1, h2, h3, h4, h5, h6\");\n        headings.forEach((heading, index) => {\n          const uniqueId = `heading-${index}-${props.divId}`;\n          heading.id = uniqueId;\n        });\n        const tables = doc.querySelectorAll(\"table\");\n        tables.forEach(table => {\n          table.classList.add(\"custom-table\");\n        });\n        const styles = Array.from(doc.querySelectorAll('link[rel=\"stylesheet\"]'));\n        styles.forEach(link => {\n          if (!document.querySelector(`link[href=\"${link.href}\"]`)) {\n            const styleTag = document.createElement(\"link\");\n            styleTag.rel = \"stylesheet\";\n            styleTag.href = link.href;\n            document.head.appendChild(styleTag);\n          }\n        });\n        const contentElement = doc.querySelector(\"body\");\n        if (!contentElement) throw new Error(\"Content not found\");\n        pageHtml.value = contentElement.innerHTML;\n        injectWikipediaStyles();\n        bus.emit(`${props.divId}_FullContent`, {\n          content: pageHtml.value,\n          title: props.pageTitle\n        });\n      } catch (err) {\n        error.value = \"Error fetching Wikipedia content: \" + err.message;\n        console.error(\"获取维基百科内容失败:\", err);\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // 原有cleanWikipediaContent函数保持不变\n    const cleanWikipediaContent = html => {\n      const parser = new DOMParser();\n      const doc = parser.parseFromString(html, \"text/html\");\n      const selectorsToRemove = [\".infobox\", \".sidebar\", \".toc\", \".reflist\", \".hatnote\", \".navbox\", \".mw-editsection\", \"table.ambox\", \".shortdescription\", \"img\", \"figure\"];\n      selectorsToRemove.forEach(selector => {\n        doc.querySelectorAll(selector).forEach(el => el.remove());\n      });\n      const bodyContent = doc.querySelector(\"#bodyContent, #mw-content-text\") || doc.body;\n      let plainText = bodyContent.textContent || bodyContent.innerText;\n      plainText = plainText.replace(/\\s+/g, \" \").replace(/\\[\\d+\\]/g, \"\").trim();\n      return plainText;\n    };\n\n    // 原有injectWikipediaStyles函数保持不变\n    const injectWikipediaStyles = () => {\n      const wikipediaStyles = [\"https://en.wikipedia.org/w/load.php?modules=site.styles\", \"https://en.wikipedia.org/w/load.php?modules=ext.cite.styles\", \"https://en.wikipedia.org/w/load.php?modules=ext.scribunto.styles\", \"https://en.wikipedia.org/w/load.php?modules=skins.vector.styles\"];\n      wikipediaStyles.forEach(url => {\n        if (!document.querySelector(`link[href=\"${url}\"]`)) {\n          const styleTag = document.createElement(\"link\");\n          styleTag.rel = \"stylesheet\";\n          styleTag.href = url;\n          document.head.appendChild(styleTag);\n        }\n      });\n    };\n\n    // 新增：精确高亮匹配函数\n    const highlightExactMatches = () => {\n      if (!divRef.value || !highlightMap.value[props.divId]) return;\n      clearHighlights(props.divId);\n      const doc = new DOMParser().parseFromString(`<div>${pageHtml.value}</div>`, \"text/html\");\n      highlightMap.value[props.divId].forEach(text => {\n        const normalizedText = text.trim().replace(/\\s+/g, \" \").replace(/\\n/g, \"\");\n        const walker = doc.createTreeWalker(doc.body, NodeFilter.SHOW_TEXT, null);\n        while (walker.nextNode()) {\n          const node = walker.currentNode;\n          const parent = node.parentNode;\n          const nodeText = node.textContent.trim().replace(/\\s+/g, \" \").replace(/\\n/g, \"\");\n          if (nodeText.includes(normalizedText)) {\n            const startIdx = nodeText.indexOf(normalizedText);\n            const endIdx = startIdx + normalizedText.length;\n            const before = node.splitText(startIdx);\n            const highlightText = before.splitText(normalizedText.length);\n            const span = doc.createElement(\"span\");\n            span.className = \"exact-text-highlight\";\n            span.dataset.highlightId = props.divId;\n            span.appendChild(before.cloneNode(true));\n            parent.replaceChild(span, before);\n            parent.insertBefore(highlightText, span.nextSibling);\n            highlightedElements.value[props.divId] = highlightedElements.value[props.divId] || [];\n            highlightedElements.value[props.divId].push(span);\n          }\n        }\n      });\n      pageHtml.value = doc.body.innerHTML;\n    };\n\n    // 新增：设置高亮内容\n    const setHighlightedTexts = (texts, targetDivId) => {\n      highlightMap.value[targetDivId] = texts;\n      if (props.divId === targetDivId && pageHtml.value) {\n        nextTick(highlightExactMatches);\n      }\n    };\n\n    // 新增：清除高亮\n    const clearHighlights = targetDivId => {\n      if (!divRef.value) return;\n      const highlights = divRef.value.querySelectorAll(`.exact-text-highlight[data-highlight-id=\"${targetDivId}\"]`);\n      highlights.forEach(el => {\n        const parent = el.parentNode;\n        parent.replaceChild(document.createTextNode(el.textContent), el);\n      });\n      highlightedElements.value[targetDivId] = [];\n    };\n\n    // 原有handleHoverInfobox等函数保持不变\n    const handleHoverInfobox = _ref => {\n      let {\n        fieldKey,\n        infoboxTitle\n      } = _ref;\n      if (props.pageTitle !== infoboxTitle) return;\n      const infobox = divRef.value.querySelector(\".infobox, .sidebar, .toccolours\");\n      if (!infobox) return;\n      clearInfoboxHighlights();\n      const rows = infobox.querySelectorAll(\"tr\");\n      for (const row of rows) {\n        const th = row.querySelector(\"th\");\n        if (th && th.textContent.trim() === fieldKey) {\n          row.classList.add(\"infobox-highlight\");\n          break;\n        }\n      }\n    };\n\n    // 其他原有函数保持不变...\n    // (包括handleUnhoverInfobox, handleSelection, closePopup等)\n\n    // 监听内容变化\n    watch(() => pageHtml.value, () => {\n      if (highlightMap.value[props.divId]) {\n        nextTick(highlightExactMatches);\n      }\n    });\n    onMounted(() => {\n      fetchWikipediaContent();\n      if (divRef.value) {\n        divRef.value.addEventListener(\"scroll\", updatePopupPosition);\n      }\n      const hoverEvent = `hover-${props.divId === \"div1\" ? \"left\" : \"right\"}-infobox`;\n      const unhoverEvent = `unhover-${props.divId === \"div1\" ? \"left\" : \"right\"}-infobox`;\n      bus.on(hoverEvent, handleHoverInfobox);\n      bus.on(unhoverEvent, handleUnhoverInfobox);\n      bus.on(`${props.divId}Event`, _ref2 => {\n        let {\n          content,\n          sourceId\n        } = _ref2;\n        selectedText.value[props.divId] = content;\n        showPopup.value[props.divId] = true;\n        nextTick(() => {\n          updatePopupPosition();\n        });\n      });\n      bus.on(\"highlight-infobox\", highlightInfoboxFields);\n      bus.on(\"unhighlight-infobox\", clearInfoboxHighlights);\n    });\n    onUnmounted(() => {\n      if (divRef.value) {\n        divRef.value.removeEventListener(\"scroll\", updatePopupPosition);\n      }\n      const hoverEvent = `hover-${props.divId === \"div1\" ? \"left\" : \"right\"}-infobox`;\n      const unhoverEvent = `unhover-${props.divId === \"div1\" ? \"left\" : \"right\"}-infobox`;\n      bus.off(hoverEvent, handleHoverInfobox);\n      bus.off(unhoverEvent, handleUnhoverInfobox);\n      bus.off(`${props.divId}Event`);\n      bus.off(\"highlight-infobox\", highlightInfoboxFields);\n      bus.off(\"unhighlight-infobox\", clearInfoboxHighlights);\n    });\n    return {\n      props,\n      pageHtml,\n      loading,\n      error,\n      selectedText,\n      showPopup,\n      divRef,\n      setHighlightedTexts,\n      // 暴露新方法\n      fetchWikipediaContent,\n      handleSelection,\n      closePopup,\n      updatePopupPosition\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","nextTick","watch","LoadingSpinner","ErrorDisplay","WikipediaContent","TextPopup","ArticleOutline","d3","bus","parseInfoboxForComparison","components","props","pageTitle","String","divId","selectContentClass","linkedOutline","Array","setup","pageHtml","loading","error","selectedText","showPopup","divRef","MainTextContent","highlightMap","highlightedElements","fetchWikipediaContent","apiUrl","encodeURIComponent","response","fetch","ok","Error","html","text","value","cleanWikipediaContent","parser","DOMParser","doc","parseFromString","infobox","querySelector","comparisonData","sidebar","remove","emit","headings","querySelectorAll","forEach","heading","index","uniqueId","id","tables","table","classList","add","styles","from","link","document","href","styleTag","createElement","rel","head","appendChild","contentElement","innerHTML","injectWikipediaStyles","content","title","err","message","console","selectorsToRemove","selector","el","bodyContent","body","plainText","textContent","innerText","replace","trim","wikipediaStyles","url","highlightExactMatches","clearHighlights","normalizedText","walker","createTreeWalker","NodeFilter","SHOW_TEXT","nextNode","node","currentNode","parent","parentNode","nodeText","includes","startIdx","indexOf","endIdx","length","before","splitText","highlightText","span","className","dataset","highlightId","cloneNode","replaceChild","insertBefore","nextSibling","push","setHighlightedTexts","texts","targetDivId","highlights","createTextNode","handleHoverInfobox","_ref","fieldKey","infoboxTitle","clearInfoboxHighlights","rows","row","th","addEventListener","updatePopupPosition","hoverEvent","unhoverEvent","on","handleUnhoverInfobox","_ref2","sourceId","highlightInfoboxFields","removeEventListener","off","handleSelection","closePopup"],"sources":["D:\\vue_frame\\vue_frame\\wikitable-vue\\client\\src\\components\\compoents_base\\ParentComponent.vue"],"sourcesContent":["<template>\r\n\t<div :class=\"['div0', selectContentClass]\" :id=\"divId\" ref=\"divRef\">\r\n\t\t<h1>{{ props.pageTitle }}</h1>\r\n\r\n\t\t