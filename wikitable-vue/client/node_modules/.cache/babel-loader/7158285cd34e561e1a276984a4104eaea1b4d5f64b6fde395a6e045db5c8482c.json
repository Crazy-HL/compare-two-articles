an\r\n\t\t\t\t\t\t\tclass=\"icon-btn compare\"\r\n\t\t\t\t\t\t\ttitle=\"ÂØπÊØîÂàÜÊûê\"\r\n\t\t\t\t\t\t\t@click=\"handleMiddleColumnClick(field)\">\r\n\t\t\t\t\t\t\t‚öñÔ∏è\r\n\t\t\t\t\t\t</span>\r\n\t\t\t\t\t\t<span\r\n\t\t\t\t\t\t\tclass=\"icon-btn merge\"\r\n\t\t\t\t\t\t\ttitle=\"ÂêàÂπ∂ÂõæË°®\"\r\n\t\t\t\t\t\t\t@click=\"showCombinedChart(field)\">\r\n\t\t\t\t\t\t\tüìä\r\n\t\t\t\t\t\t</span>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div\r\n\t\t\t\t\tclass=\"cell right-column\"\r\n\t\t\t\t\t@mouseover=\"hoverInfobox(rightInfobox, field.key, 'right')\"\r\n\t\t\t\t\t@mouseout=\"unhoverInfobox('right')\"\r\n\t\t\t\t\t@click=\"showFullChart(rightInfobox, field)\">\r\n\t\t\t\t\t<SimpleChart\r\n\t\t\t\t\t\t:field=\"getField(rightInfobox, field.key)\"\r\n\t\t\t\t\t\t:type=\"field.type\"\r\n\t\t\t\t\t\t:visualization=\"field.visualization\" />\r\n\t\t\t\t</div>\r\n\t\t\t</template>\r\n\t\t</div>\r\n\r\n\t\t<!-- ÂÖ®Â±èÂõæË°®Ê®°ÊÄÅÊ°Ü -->\r\n\t\t<div\r\n\t\t\tv-if=\"showFullChartModal\"\r\n\t\t\tclass=\"full-chart-modal\"\r\n\t\t\t@click.self=\"closeFullChart\">\r\n\t\t\t<div class=\"modal-content\">\r\n\t\t\t\t<button class=\"close-btn\" @click=\"closeFullChart\">√ó</button>\r\n\t\t\t\t<h3>{{ currentChart.title }} - {{ currentChart.field.key }}</h3>\r\n\t\t\t\t<div class=\"chart-container\">\r\n\t\t\t\t\t<FullChart\r\n\t\t\t\t\t\t:field=\"currentChart.data\"\r\n\t\t\t\t\t\t:type=\"currentChart.field.type\"\r\n\t\t\t\t\t\t:visualization=\"currentChart.field.visualization\" />\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"chart-legend\" v-if=\"currentChart.field.legend\">\r\n\t\t\t\t\t{{ currentChart.field.legend }}\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script setup>\r\n\t\timport { ref, computed, onMounted, watch, onUnmounted } from \"vue\";\r\n\t\timport SimpleChart from \"./SimpleChart.vue\";\r\n\t\timport FullChart from \"./FullChart.vue\";\r\n\t\timport bus from \"@/js/eventBus.js\";\r\n\r\n\t\tconst props = defineProps({\r\n\t\t\tdiv1RawData: Object,\r\n\t\t\tdiv3RawData: Object\r\n\t\t});\r\n\r\n\t\tconst emit = defineEmits([\"compareAttribute\"]);\r\n\r\n\t\t// Áä∂ÊÄÅÂèòÈáè\r\n\t\tconst leftInfobox = ref({ title: \"\", type: \"\", data: {} });\r\n\t\tconst rightInfobox = ref({ title: \"\", type: \"\", data: {} });\r\n\t\tconst showFullChartModal = ref(false);\r\n\t\tconst currentChart = ref({\r\n\t\t\ttitle: \"\",\r\n\t\t\tfield: {},\r\n\t\t\tdata: []\r\n\t\t});\r\n\t\tconst showSortingHeatmap = ref(true);\r\n\t\tconst leftDataLoaded = ref(false);\r\n\t\tconst rightDataLoaded = ref(false);\r\n\t\tconst sortedFieldsWithScores = ref([]);\r\n\r\n\t\t// ÂèØÊØîËæÉÂ≠óÊÆµÈÖçÁΩÆ\r\n\t\tconst COMPARABLE_FIELDS = [\r\n\t\t\t{\r\n\t\t\t\tkey: \"GDP\",\r\n\t\t\t\ttype: \"number\",\r\n\t\t\t\ttypeLabel: \"Êï∞ÂÄº(ÁæéÂÖÉ)\",\r\n\t\t\t\tvisualization: \"bar-chart\",\r\n\t\t\t\tlegend: \"ÂõΩÂÜÖÁîü‰∫ßÊÄªÂÄºÔºàÂçï‰ΩçÔºö‰∏á‰∫øÁæéÂÖÉÔºâ\"\r\n\t\t\t},\r\n\t\t\t{\r\n\t\t\t\tkey: \"Population\",\r\n\t\t\t\ttype: \"number\",\r\n\t\t\t\ttypeLabel: \"Êï∞ÂÄº(‰∫∫)\",\r\n\t\t\t\tvisualization: \"bar-chart\",\r\n\t\t\t\tlegend: \"‰∫∫Âè£Êï∞ÈáèÔºàÂçï‰ΩçÔºö‰∫ø‰∫∫Ôºâ\"\r\n\t\t\t},\r\n\t\t\t{\r\n\t\t\t\tkey: \"GDP growth\",\r\n\t\t\t\ttype: \"percentage\",\r\n\t\t\t\ttypeLabel: \"ÁôæÂàÜÊØî(%)\",\r\n\t\t\t\tvisualization: \"line-chart\",\r\n\t\t\t\tlegend: \"GDPÂπ¥Â¢ûÈïøÁéáÔºà%Ôºâ\"\r\n\t\t\t},\r\n\t\t\t{\r\n\t\t\t\tkey: \"Inflation (CPI)\",\r\n\t\t\t\ttype: \"percentage\",\r\n\t\t\t\ttypeLabel: \"ÁôæÂàÜÊØî(%)\",\r\n\t\t\t\tvisualization: \"pie-chart\",\r\n\t\t\t\tlegend: \"Ê∂àË¥πËÄÖ‰ª∑Ê†ºÊåáÊï∞ÂèòÂåñ\"\r\n\t\t\t},\r\n\r\n\t\t\t{\r\n\t\t\t\tkey: \"GDP rank\",\r\n\t\t\t\ttype: \"text\",\r\n\t\t\t\ttypeLabel: \"ÊñáÊú¨\",\r\n\t\t\t\tvisualization: \"text-only\",\r\n\t\t\t\tlegend: \"ÂÖ®ÁêÉGDPÊéíÂêç\"\r\n\t\t\t}\r\n\t\t];\r\n\r\n\t\t// ‰øÆÊîπÂêéÁöÑgetFieldÊñπÊ≥ï\r\n\t\tconst getField = (infobox, fieldKey) => {\r\n\t\t\tif (!infobox?.data) {\r\n\t\t\t\tconsole.warn(`InfoboxÊï∞ÊçÆÊú™Âä†ËΩΩ: ${infobox.title}`);\r\n\t\t\t\treturn [];\r\n\t\t\t}\r\n\r\n\t\t\tfor (const section of Object.values(infobox.data)) {\r\n\t\t\t\tif (section[fieldKey] !== undefined) {\r\n\t\t\t\t\tconst fieldData = section[fieldKey];\r\n\r\n\t\t\t\t\t// Â¶ÇÊûúÊòØÊï∞ÁªÑÔºàÂ¶ÇÂàóË°®È°πÔºâÔºåÊèêÂèñvalueÊàñÂéüÂßãÂÄº\r\n\t\t\t\t\tif (Array.isArray(fieldData)) {\r\n\t\t\t\t\t\treturn fieldData.map(item => {\r\n\t\t\t\t\t\t\t// Â¶ÇÊûúÊòØÂØπË±°ÔºàÊñ∞Ëß£ÊûêÂô®ÁªìÊûÑÔºâÔºå‰ºòÂÖàËøîÂõûvalueÂ≠óÊÆµ\r\n\t\t\t\t\t\t\tif (typeof item === \"object\" && item !== null) {\r\n\t\t\t\t\t\t\t\treturn item.value ?? item.raw ?? item;\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\treturn item;\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\r\n\t\t\t\t\t// Â¶ÇÊûúÊòØÂØπË±°ÔºàÊñ∞Ëß£ÊûêÂô®ÁªìÊûÑÔºâÔºåÊèêÂèñvalueÊàñÂéüÂßãÂÄº\r\n\t\t\t\t\tif (typeof fieldData === \"object\" && fieldData !== null) {\r\n\t\t\t\t\t\treturn [fieldData.value ?? fieldData.raw ?? fieldData];\r\n\t\t\t\t\t}\r\n\r\n\t\t\t\t\t// ÊóßÊ†ºÂºèÁõ¥Êé•ËøîÂõû\r\n\t\t\t\t\treturn [fieldData];\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\treturn [];\r\n\t\t};\r\n\r\n\t\tconst calculateDifferenceScore = (field) => {\r\n\t  const leftData = getField(leftInfobox.value, field.key);\r\n\t  const rightData = getField(rightInfobox.value, field.key);\r\n\r\n\t  if (!leftData.length || !rightData.length) return 0;\r\n\r\n\t  // ÊèêÂèñÂÆûÈôÖÂÄºÔºàÂÖºÂÆπÊñ∞ÊóßÊï∞ÊçÆÁªìÊûÑÔºâ\r\n\t  const getComparableValue = (data) => {\r\n\t    const val = data[0];\r\n\t    if (typeof val === 'object' && val !== null && 'value' in val) {\r\n\t      return val.value;\r\n\t    }\r\n\t    return val;\r\n\t  };\r\n\r\n\t  const leftVal = getComparableValue(leftData);\r\n\t  const rightVal = getComparableValue(rightData);\r\n\r\n\t  // ÊñáÊú¨Á±ªÂûãÊØîËæÉ\r\n\t  if (field.type === \"text\") {\r\n\t    const isSame = String(leftVal).trim() === String(rightVal).trim();\r\n\t    return isSame ? 1 : 5; // ÊñáÊú¨Â∑ÆÂºÇÂõ∫ÂÆöÂàÜÊï∞\r\n\t  }\r\n\r\n\t  // Êï∞ÂÄºÁ±ªÂûãÊØîËæÉ\r\n\t  if (field.type === \"number\" || field.type === \"percentage\") {\r\n\t    const leftNum = parseFloat(leftVal);\r\n\t    const rightNum = parseFloat(rightVal);\r\n\r\n\t    if (isNaN(leftNum) || isNaN(rightNum)) return 0;\r\n\r\n\t    // Ê†∏ÂøÉËÆ°ÁÆóÈÄªËæë\r\n\t    const absLeft = Math.abs(leftNum);\r\n\t    const absRight = Math.abs(rightNum);\r\n\t    const avg = (absLeft + absRight) / 2;\r\n\t    const absDiff = Math.abs(leftNum - rightNum);\r\n\t    const relativeDiff = avg > 0 ? absDiff / avg : 0;\r\n\r\n\t    // Ë∂ãÂäøÊ£ÄÊµãÔºàÊ†∏ÂøÉ‰øÆÊîπÔºâ\r\n\t    const isOppositeTrend =\r\n\t      (leftNum > 0 && rightNum < 0) ||\r\n\t      (leftNum < 0 && rightNum > 0);\r\n\r\n\t    // ÂàÜÊï∞ËÆ°ÁÆó\r\n\t    let score;\r\n\t    if (isOppositeTrend) {\r\n\t      // Ë∂ãÂäøÁõ∏ÂèçÔºöÂü∫Á°ÄÂàÜ60 + Â∑ÆÂºÇÂä†ÊàêÔºà0-40Ôºâ\r\n\t      score = 60 + Math.min(40, relativeDiff * 100);\r\n\t    } else {\r\n\t      // Ë∂ãÂäøÁõ∏ÂêåÔºöÂü∫Á°ÄÂàÜ10 + Â∑ÆÂºÇÂä†ÊàêÔºà0-50Ôºâ\r\n\t      score = 10 + Math.min(50, relativeDiff * 50);\r\n\t    }\r\n\r\n\t    // Â∫îÁî®ÊùÉÈáç\r\n\t    const weight = getFieldWeight(field.key);\r\n\t    score *= weight;\r\n\r\n\t    // ÈôêÂà∂ÂàÜÊï∞ËåÉÂõ¥Âπ∂ÂõõËàç‰∫îÂÖ•\r\n\t    return Math.min(100, Math.max(0, Math.round(score * 10) / 10);\r\n\t  }\r\n\r\n\t  return 0;\r\n\t};\r\n\r\n\t\t// Â≠óÊÆµÊùÉÈáç‰πüÈúÄË¶ÅÁõ∏Â∫îË∞ÉÊï¥ÔºàÊõ¥ÂêàÁêÜÁöÑËåÉÂõ¥Ôºâ\r\n\t\tconst getFieldWeight = fieldKey => {\r\n\t\t\tconst weights = {\r\n\t\t\t\tGDP: 1.5,\r\n\t\t\t\tPopulation: 1.3,\r\n\t\t\t\t\"GDP growth\": 2.0, // Â¢ûÈïøÁéáÂ∑ÆÂºÇÊõ¥ÈáçË¶Å\r\n\t\t\t\tInflation: 1.8,\r\n\t\t\t\tLabor: 1.2\r\n\t\t\t};\r\n\r\n\t\t\tfor (const [key, weight] of Object.entries(weights)) {\r\n\t\t\t\tif (fieldKey.toLowerCase().includes(key.toLowerCase())) {\r\n\t\t\t\t\treturn weight;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\r\n\t\t\treturn 1.0; // ÈªòËÆ§ÊùÉÈáç\r\n\t\t};\r\n\r\n\t\t// ÁÉ≠ÂäõÂõæÈ¢úËâ≤\r\n\t\tconst getHeatmapColor = score => {\r\n\t\t\tconst maxScore = 100;\r\n\t\t\tconst ratio = Math.min(score / maxScore, 1);\r\n\t\t\tconst hue = (1 - ratio) * 120; // ÁªøËâ≤(120)Âà∞Á∫¢Ëâ≤(0)