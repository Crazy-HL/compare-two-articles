{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, onUnmounted } from \"vue\";\nimport bus from \"@/js/eventBus.js\";\nimport { renderLineChart, renderBarChart, renderNonVisualChart } from \"@/js/chartUtils\";\n\n// 使用数组存储图表引用\n\nexport default {\n  __name: 'Div2',\n  setup(__props, _ref) {\n    let {\n      expose: __expose\n    } = _ref;\n    __expose();\n    const chart1Refs = ref([]);\n    const chart2Refs = ref([]);\n\n    // 响应式数据\n    const userQuestion = ref(\"\");\n    const chatHistory = ref([]);\n    const selectText2 = ref(\"\");\n    const selectText3 = ref(\"\");\n    const article1Data = ref(null);\n    const article2Data = ref(null);\n    const commonAttributes = ref([]);\n    const selectedAttributeIndex = ref(-1);\n\n    // 获取空表格提示信息\n    const getEmptyTableMessage = () => {\n      if (!selectText2.value && !selectText3.value) return \"请先选择两篇文章\";\n      if (!selectText2.value) return \"请先选择文章1\";\n      if (!selectText3.value) return \"请先选择文章2\";\n      return \"无公共属性可对比\";\n    };\n\n    // 安全的文本提取\n    const getPlainTextFromSelection = htmlContent => {\n      if (!htmlContent) return \"\";\n      try {\n        const container = document.createElement(\"div\");\n        container.innerHTML = htmlContent;\n        return container.textContent || container.innerText || \"\";\n      } catch (error) {\n        console.error(\"文本提取错误:\", error);\n        return \"\";\n      }\n    };\n\n    // 事件处理\n    const handleSelection = (data, source) => {\n      if (!data?.content) {\n        console.warn(`无效数据来自 ${source}:`, data);\n        return;\n      }\n      const plainText = getPlainTextFromSelection(data.content);\n      if (source === \"div1\") {\n        selectText2.value = plainText;\n      } else if (source === \"div3\") {\n        selectText3.value = plainText;\n      }\n    };\n\n    // 选择属性\n    const selectAttribute = (attr, index) => {\n      if (!attr || index < 0 || index >= commonAttributes.value.length) {\n        console.warn(\"无效属性选择:\", attr, index);\n        return;\n      }\n      selectedAttributeIndex.value = index;\n      renderCharts(index);\n    };\n\n    // 渲染图表\n    const renderCharts = index => {\n      if (index < 0 || !article1Data.value || !article2Data.value) return;\n      const attr = commonAttributes.value[index];\n      if (!attr) return;\n      try {\n        const chart1El = chart1Refs.value[index];\n        const chart2El = chart2Refs.value[index];\n        if (chart1El && article1Data.value[attr]) {\n          renderChart(chart1El, article1Data.value[attr], \"Line Chart\");\n        }\n        if (chart2El && article2Data.value[attr]) {\n          renderChart(chart2El, article2Data.value[attr], \"Line Chart\");\n        }\n      } catch (error) {\n        console.error(\"图表渲染错误:\", error);\n      }\n    };\n\n    // 安全渲染图表\n    const renderChart = (container, data, type) => {\n      if (!container || !data) return;\n      try {\n        // 先清空容器\n        container.innerHTML = \"\";\n        switch (type) {\n          case \"Line Chart\":\n            renderLineChart(container, data.data || [], data.options || {});\n            break;\n          case \"Bar Chart\":\n            renderBarChart(container, data);\n            break;\n          default:\n            renderNonVisualChart(container, null, {\n              message: \"不支持的图表类型\"\n            });\n        }\n      } catch (error) {\n        console.error(\"图表渲染失败:\", error);\n        renderNonVisualChart(container, null, {\n          message: \"渲染出错\"\n        });\n      }\n    };\n\n    // 对比属性\n    const compareAttributes = async () => {\n      if (!selectText2.value || !selectText3.value) {\n        alert(\"请先选择两段文本！\");\n        return;\n      }\n      try {\n        const response = await api.post(\"compare_attributes\", {\n          text1: selectText2.value,\n          text2: selectText3.value\n        });\n        if (!response || response.error) {\n          throw new Error(response?.error || \"请求失败\");\n        }\n        article1Data.value = response.article1 || {};\n        article2Data.value = response.article2 || {};\n        commonAttributes.value = response.common_attributes || [];\n        selectedAttributeIndex.value = -1;\n\n        // 重置图表引用数组\n        chart1Refs.value = [];\n        chart2Refs.value = [];\n        if (commonAttributes.value.length > 0) {\n          // 默认选择第一个属性\n          nextTick(() => {\n            selectAttribute(commonAttributes.value[0], 0);\n          });\n        }\n      } catch (error) {\n        console.error(\"属性对比失败:\", error);\n        chatHistory.value.push({\n          role: \"assistant\",\n          content: `属性对比失败: ${error.message}`\n        });\n      }\n    };\n\n    // 事件监听\n    onMounted(() => {\n      bus.on(\"div1_Event\", data => handleSelection(data, \"div1\"));\n      bus.on(\"div3_Event\", data => handleSelection(data, \"div3\"));\n    });\n    onUnmounted(() => {\n      bus.off(\"div1_Event\");\n      bus.off(\"div3_Event\");\n    });\n    const __returned__ = {\n      chart1Refs,\n      chart2Refs,\n      userQuestion,\n      chatHistory,\n      selectText2,\n      selectText3,\n      article1Data,\n      article2Data,\n      commonAttributes,\n      selectedAttributeIndex,\n      getEmptyTableMessage,\n      getPlainTextFromSelection,\n      handleSelection,\n      selectAttribute,\n      renderCharts,\n      renderChart,\n      compareAttributes,\n      ref,\n      onMounted,\n      onUnmounted,\n      get bus() {\n        return bus;\n      },\n      get renderLineChart() {\n        return renderLineChart;\n      },\n      get renderBarChart() {\n        return renderBarChart;\n      },\n      get renderNonVisualChart() {\n        return renderNonVisualChart;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","bus","renderLineChart","renderBarChart","renderNonVisualChart","chart1Refs","chart2Refs","userQuestion","chatHistory","selectText2","selectText3","article1Data","article2Data","commonAttributes","selectedAttributeIndex","getEmptyTableMessage","value","getPlainTextFromSelection","htmlContent","container","document","createElement","innerHTML","textContent","innerText","error","console","handleSelection","data","source","content","warn","plainText","selectAttribute","attr","index","length","renderCharts","chart1El","chart2El","renderChart","type","options","message","compareAttributes","alert","response","api","post","text1","text2","Error","article1","article2","common_attributes","nextTick","push","role","on","off"],"sources":["D:/vue_frame/vue_frame/wikitable-vue/client/src/components/Div2.vue"],"sourcesContent":["<template>\r\n\t<div class=\"main-container\">\r\n\t\t<!-- 聊天容器 -->\r\n\t\t<div class=\"chat-container\" @dragover.prevent @drop=\"handleDrop\">\r\n\t\t\t<!-- 历史对话记录 -->\r\n\t\t\t<div class=\"chat-history\">\r\n\t\t\t\t<template v-for=\"(message, index) in chatHistory\" :key=\"index\">\r\n\t\t\t\t\t<div v-if=\"message?.content\" :class=\"['message', message.role]\">\r\n\t\t\t\t\t\t<div class=\"message-content\">\r\n\t\t\t\t\t\t\t<strong>{{ message.role === \"user\" ? \"用户\" : \"GPT\" }}:</strong>\r\n\t\t\t\t\t\t\t<p v-html=\"message.content\"></p>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</template>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\t\t<!-- 视觉内容容器 -->\r\n\t\t<div class=\"vis-container\">\r\n\t\t\t<!-- 三列对比表格 -->\r\n\t\t\t<div class=\"comparison-table\">\r\n\t\t\t\t<!-- 表头 -->\r\n\t\t\t\t<div class=\"table-header\">\r\n\t\t\t\t\t<div class=\"header-cell\">文章1属性可视化</div>\r\n\t\t\t\t\t<div class=\"header-cell\">公共属性</div>\r\n\t\t\t\t\t<div class=\"header-cell\">文章2属性可视化</div>\r\n\t\t\t\t</div>\r\n\r\n\t\t\t\t<!-- 表格内容 -->\r\n\t\t\t\t<div class=\"table-body\">\r\n\t\t\t\t\t<template v-if=\"commonAttributes.length > 0\">\r\n\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\tv-for=\"(attr, index) in commonAttributes\"\r\n\t\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t\tclass=\"table-row\">\r\n\t\t\t\t\t\t\t<!-- 文章1图表 -->\r\n\t\t\t\t\t\t\t<div class=\"table-cell chart-cell\">\r\n\t\t\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\t\t\t:ref=\"\r\n\t\t\t\t\t\t\t\t\t\tel => {\r\n\t\t\t\t\t\t\t\t\t\t\tif (el) chart1Refs[index] = el;\r\n\t\t\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t\t\t\"\r\n\t\t\t\t\t\t\t\t\tclass=\"chart-box\"></div>\r\n\t\t\t\t\t\t\t</div>\r\n\r\n\t\t\t\t\t\t\t<!-- 公共属性名称 -->\r\n\t\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\t\tclass=\"table-cell attr-cell\"\r\n\t\t\t\t\t\t\t\t@click=\"selectAttribute(attr, index)\">\r\n\t\t\t\t\t\t\t\t{{ attr }}\r\n\t\t\t\t\t\t\t</div>\r\n\r\n\t\t\t\t\t\t\t<!-- 文章2图表 -->\r\n\t\t\t\t\t\t\t<div class=\"table-cell chart-cell\">\r\n\t\t\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\t\t\t:ref=\"\r\n\t\t\t\t\t\t\t\t\t\tel => {\r\n\t\t\t\t\t\t\t\t\t\t\tif (el) chart2Refs[index] = el;\r\n\t\t\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t\t\t\"\r\n\t\t\t\t\t\t\t\t\tclass=\"chart-box\"></div>\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</template>\r\n\r\n\t\t\t\t\t<!-- 空状态 -->\r\n\t\t\t\t\t<div v-else class=\"empty-table\">\r\n\t\t\t\t\t\t{{ getEmptyTableMessage() }}\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\r\n\t\t\t<!-- 输入框和操作按钮 -->\r\n\t\t\t<div class=\"input-container\">\r\n\t\t\t\t<textarea\r\n\t\t\t\t\tv-model=\"userQuestion\"\r\n\t\t\t\t\trows=\"2\"\r\n\t\t\t\t\tplaceholder=\"请输入你想问的问题...\"></textarea>\r\n\t\t\t\t<div class=\"button-container\">\r\n\t\t\t\t\t<button @click=\"askQuestion\">发送</button>\r\n\t\t\t\t\t<button\r\n\t\t\t\t\t\t@click=\"compareAttributes\"\r\n\t\t\t\t\t\tclass=\"submit-btn\"\r\n\t\t\t\t\t\t:disabled=\"!selectText2 || !selectText3\">\r\n\t\t\t\t\t\t属性对比可视化\r\n\t\t\t\t\t</button>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script setup>\r\n\timport { ref, onMounted, onUnmounted } from \"vue\";\r\n\timport bus from \"@/js/eventBus.js\";\r\n\timport {\r\n\t\trenderLineChart,\r\n\t\trenderBarChart,\r\n\t\trenderNonVisualChart\r\n\t} from \"@/js/chartUtils\";\r\n\r\n\t// 使用数组存储图表引用\r\n\tconst chart1Refs = ref([]);\r\n\tconst chart2Refs = ref([]);\r\n\r\n\t// 响应式数据\r\n\tconst userQuestion = ref(\"\");\r\n\tconst chatHistory = ref([]);\r\n\tconst selectText2 = ref(\"\");\r\n\tconst selectText3 = ref(\"\");\r\n\tconst article1Data = ref(null);\r\n\tconst article2Data = ref(null);\r\n\tconst commonAttributes = ref([]);\r\n\tconst selectedAttributeIndex = ref(-1);\r\n\r\n\t// 获取空表格提示信息\r\n\tconst getEmptyTableMessage = () => {\r\n\t\tif (!selectText2.value && !selectText3.value) return \"请先选择两篇文章\";\r\n\t\tif (!selectText2.value) return \"请先选择文章1\";\r\n\t\tif (!selectText3.value) return \"请先选择文章2\";\r\n\t\treturn \"无公共属性可对比\";\r\n\t};\r\n\r\n\t// 安全的文本提取\r\n\tconst getPlainTextFromSelection = htmlContent => {\r\n\t\tif (!htmlContent) return \"\";\r\n\t\ttry {\r\n\t\t\tconst container = document.createElement(\"div\");\r\n\t\t\tcontainer.innerHTML = htmlContent;\r\n\t\t\treturn container.textContent || container.innerText || \"\";\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"文本提取错误:\", error);\r\n\t\t\treturn \"\";\r\n\t\t}\r\n\t};\r\n\r\n\t// 事件处理\r\n\tconst handleSelection = (data, source) => {\r\n\t\tif (!data?.content) {\r\n\t\t\tconsole.warn(`无效数据来自 ${source}:`, data);\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\tconst plainText = getPlainTextFromSelection(data.content);\r\n\t\tif (source === \"div1\") {\r\n\t\t\tselectText2.value = plainText;\r\n\t\t} else if (source === \"div3\") {\r\n\t\t\tselectText3.value = plainText;\r\n\t\t}\r\n\t};\r\n\r\n\t// 选择属性\r\n\tconst selectAttribute = (attr, index) => {\r\n\t\tif (!attr || index < 0 || index >= commonAttributes.value.length) {\r\n\t\t\tconsole.warn(\"无效属性选择:\", attr, index);\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\tselectedAttributeIndex.value = index;\r\n\t\trenderCharts(index);\r\n\t};\r\n\r\n\t// 渲染图表\r\n\tconst renderCharts = index => {\r\n\t\tif (i