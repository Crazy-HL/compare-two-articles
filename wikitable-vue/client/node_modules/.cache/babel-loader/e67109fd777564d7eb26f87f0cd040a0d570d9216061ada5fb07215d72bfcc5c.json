{"ast":null,"code":"import { ref, onMounted, watch } from \"vue\";\nimport * as d3 from \"d3\";\nexport default {\n  __name: 'FullChart',\n  props: {\n    field: Array,\n    type: String,\n    visualization: String\n  },\n  setup(__props, _ref) {\n    let {\n      expose: __expose\n    } = _ref;\n    __expose();\n    const props = __props;\n    const chartContainer = ref(null);\n    const renderChart = () => {\n      if (!chartContainer.value || !props.field || props.field.length === 0) return;\n\n      // 清空容器\n      chartContainer.value.innerHTML = \"\";\n\n      // 根据不同类型渲染图表\n      switch (props.visualization) {\n        case \"bar-chart\":\n          renderBarChart();\n          break;\n        case \"line-chart\":\n          renderLineChart();\n          break;\n        case \"pie-chart\":\n          renderPieChart();\n          break;\n        default:\n          renderTextOnly();\n      }\n    };\n    const renderBarChart = () => {\n      const width = 800;\n      const height = 500;\n      const margin = {\n        top: 40,\n        right: 40,\n        bottom: 60,\n        left: 60\n      };\n      const svg = d3.select(chartContainer.value).append(\"svg\").attr(\"width\", width).attr(\"height\", height);\n      const data = props.field.map(item => ({\n        label: item.label || item.name,\n        value: +item.value\n      }));\n      const x = d3.scaleBand().domain(data.map(d => d.label)).range([margin.left, width - margin.right]).padding(0.2);\n      const y = d3.scaleLinear().domain([0, d3.max(data, d => d.value)]).nice().range([height - margin.bottom, margin.top]);\n\n      // 添加坐标轴\n      svg.append(\"g\").attr(\"class\", \"x-axis\").attr(\"transform\", `translate(0,${height - margin.bottom})`).call(d3.axisBottom(x)).selectAll(\"text\").style(\"text-anchor\", \"end\").attr(\"dx\", \"-.8em\").attr(\"dy\", \".15em\").attr(\"transform\", \"rotate(-45)\");\n      svg.append(\"g\").attr(\"class\", \"y-axis\").attr(\"transform\", `translate(${margin.left},0)`).call(d3.axisLeft(y));\n\n      // 添加网格线\n      svg.append(\"g\").attr(\"class\", \"grid\").attr(\"transform\", `translate(${margin.left},0)`).call(d3.axisLeft(y).tickSize(-width + margin.left + margin.right).tickFormat(\"\"));\n\n      // 添加柱状图\n      svg.append(\"g\").selectAll(\"rect\").data(data).join(\"rect\").attr(\"x\", d => x(d.label)).attr(\"y\", d => y(d.value)).attr(\"width\", x.bandwidth()).attr(\"height\", d => y(0) - y(d.value)).attr(\"fill\", \"steelblue\").attr(\"rx\", 4) // 圆角\n      .attr(\"ry\", 4);\n\n      // 添加数值标签\n      svg.append(\"g\").selectAll(\"text\").data(data).join(\"text\").attr(\"x\", d => x(d.label) + x.bandwidth() / 2).attr(\"y\", d => y(d.value) - 5).attr(\"text-anchor\", \"middle\").text(d => d.value);\n\n      // 添加标题\n      svg.append(\"text\").attr(\"x\", width / 2).attr(\"y\", margin.top / 2).attr(\"text-anchor\", \"middle\").style(\"font-size\", \"16px\").style(\"font-weight\", \"bold\").text(\"数据对比图表\");\n\n      // 添加X轴标签\n      svg.append(\"text\").attr(\"x\", width / 2).attr(\"y\", height - 10).attr(\"text-anchor\", \"middle\").text(\"分类\");\n\n      // 添加Y轴标签\n      svg.append(\"text\").attr(\"transform\", \"rotate(-90)\").attr(\"y\", 10).attr(\"x\", -height / 2).attr(\"text-anchor\", \"middle\").text(\"数值\");\n    };\n    const renderLineChart = () => {\n      const width = 800;\n      const height = 500;\n      const margin = {\n        top: 40,\n        right: 40,\n        bottom: 60,\n        left: 60\n      };\n      const svg = d3.select(chartContainer.value).append(\"svg\").attr(\"width\", width).attr(\"height\", height);\n      const data = props.field.map(item => ({\n        label: item.label || item.name,\n        value: +item.value,\n        date: item.date ? new Date(item.date) : null\n      })).sort((a, b) => a.date - b.date);\n      const x = data[0].date ? d3.scaleTime().domain(d3.extent(data, d => d.date)).range([margin.left, width - margin.right]) : d3.scaleBand().domain(data.map(d => d.label)).range([margin.left, width - margin.right]).padding(0.2);\n      const y = d3.scaleLinear().domain([0, d3.max(data, d => d.value)]).nice().range([height - margin.bottom, margin.top]);\n\n      // 添加坐标轴\n      svg.append(\"g\").attr(\"class\", \"x-axis\").attr(\"transform\", `translate(0,${height - margin.bottom})`).call(d3.axisBottom(x));\n      svg.append(\"g\").attr(\"class\", \"y-axis\").attr(\"transform\", `translate(${margin.left},0)`).call(d3.axisLeft(y));\n\n      // 添加网格线\n      svg.append(\"g\").attr(\"class\", \"grid\").attr(\"transform\", `translate(${margin.left},0)`).call(d3.axisLeft(y).tickSize(-width + margin.left + margin.right).tickFormat(\"\"));\n\n      // 创建折线生成器\n      const line = d3.line().x(d => data[0].date ? x(d.date) : x(d.label) + x.bandwidth() / 2).y(d => y(d.value));\n\n      // 添加折线\n      svg.append(\"path\").datum(data).attr(\"fill\", \"none\").attr(\"stroke\", \"steelblue\").attr(\"stroke-width\", 2).attr(\"d\", line);\n\n      // 添加数据点\n      svg.append(\"g\").selectAll(\"circle\").data(data).join(\"circle\").attr(\"cx\", d => data[0].date ? x(d.date) : x(d.label) + x.bandwidth() / 2).attr(\"cy\", d => y(d.value)).attr(\"r\", 5).attr(\"fill\", \"steelblue\");\n\n      // 添加数值标签\n      svg.append(\"g\").selectAll(\"text\").data(data).join(\"text\").attr(\"x\", d => data[0].date ? x(d.date) : x(d.label) + x.bandwidth() / 2).attr(\"y\", d => y(d.value) - 10).attr(\"text-anchor\", \"middle\").text(d => d.value);\n\n      // 添加标题\n      svg.append(\"text\").attr(\"x\", width / 2).attr(\"y\", margin.top / 2).attr(\"text-anchor\", \"middle\").style(\"font-size\", \"16px\").style(\"font-weight\", \"bold\").text(\"趋势变化图表\");\n\n      // 添加X轴标签\n      svg.append(\"text\").attr(\"x\", width / 2).attr(\"y\", height - 10).attr(\"text-anchor\", \"middle\").text(data[0].date ? \"时间\" : \"分类\");\n\n      // 添加Y轴标签\n      svg.append(\"text\").attr(\"transform\", \"rotate(-90)\").attr(\"y\", 10).attr(\"x\", -height / 2).attr(\"text-anchor\", \"middle\").text(\"数值\");\n    };\n    const renderPieChart = () => {\n      const width = 600;\n      const height = 500;\n      const radius = Math.min(width, height) / 2 - 40;\n      const svg = d3.select(chartContainer.value).append(\"svg\").attr(\"width\", width).attr(\"height\", height).append(\"g\").attr(\"transform\", `translate(${width / 2},${height / 2})`);\n      const data = props.field.map(item => ({\n        label: item.label || item.name,\n        value: +item.value\n      }));\n      const color = d3.scaleOrdinal().domain(data.map(d => d.label)).range(d3.schemeCategory10);\n      const pie = d3.pie().value(d => d.value);\n      const arc = d3.arc().innerRadius(0).outerRadius(radius);\n      const arcs = svg.selectAll(\"arc\").data(pie(data)).enter().append(\"g\").attr(\"class\", \"arc\");\n\n      // 添加扇形\n      arcs.append(\"path\").attr(\"d\", arc).attr(\"fill\", d => color(d.data.label)).attr(\"stroke\", \"white\").attr(\"stroke-width\", 2);\n\n      // 添加标签\n      arcs.append(\"text\").attr(\"transform\", d => `translate(${arc.centroid(d)})`).attr(\"text-anchor\", \"middle\").text(d => d.data.value).style(\"font-size\", \"12px\").style(\"fill\", \"white\");\n\n      // 添加图例\n      const legend = svg.selectAll(\".legend\").data(data).enter().append(\"g\").attr(\"class\", \"legend\").attr(\"transform\", (d, i) => `translate(${radius + 30},${i * 20 - radius / 2})`);\n      legend.append(\"rect\").attr(\"width\", 18).attr(\"height\", 18).attr(\"fill\", d => color(d.label));\n      legend.append(\"text\").attr(\"x\", 24).attr(\"y\", 9).attr(\"dy\", \".35em\").text(d => `${d.label}: ${d.value}`).style(\"font-size\", \"12px\");\n\n      // 添加标题\n      svg.append(\"text\").attr(\"y\", -height / 2 + 20).attr(\"text-anchor\", \"middle\").style(\"font-size\", \"16px\").style(\"font-weight\", \"bold\").text(\"比例分布图表\");\n    };\n    const renderTextOnly = () => {\n      const container = d3.select(chartContainer.value);\n      container.html(\"\");\n      const data = props.field;\n      container.append(\"div\").style(\"font-size\", \"16px\").style(\"font-weight\", \"bold\").style(\"margin-bottom\", \"20px\").style(\"text-align\", \"center\").text(\"文本数据\");\n      const table = container.append(\"table\").style(\"width\", \"100%\").style(\"border-collapse\", \"collapse\");\n      const thead = table.append(\"thead\");\n      const tbody = table.append(\"tbody\");\n      thead.append(\"tr\").selectAll(\"th\").data([\"项目\", \"值\"]).enter().append(\"th\").style(\"padding\", \"8px\").style(\"border\", \"1px solid #ddd\").style(\"background-color\", \"#f2f2f2\").text(d => d);\n      const rows = tbody.selectAll(\"tr\").data(data).enter().append(\"tr\");\n      rows.append(\"td\").style(\"padding\", \"8px\").style(\"border\", \"1px solid #ddd\").text(d => d.label || d.name);\n      rows.append(\"td\").style(\"padding\", \"8px\").style(\"border\", \"1px solid #ddd\").text(d => d.value);\n    };\n    onMounted(renderChart);\n    watch(() => props.field, renderChart);\n    const __returned__ = {\n      props,\n      chartContainer,\n      renderChart,\n      renderBarChart,\n      renderLineChart,\n      renderPieChart,\n      renderTextOnly,\n      ref,\n      onMounted,\n      watch,\n      get d3() {\n        return d3;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","watch","d3","props","__props","chartContainer","renderChart","value","field","length","innerHTML","visualization","renderBarChart","renderLineChart","renderPieChart","renderTextOnly","width","height","margin","top","right","bottom","left","svg","select","append","attr","data","map","item","label","name","x","scaleBand","domain","d","range","padding","y","scaleLinear","max","nice","call","axisBottom","selectAll","style","axisLeft","tickSize","tickFormat","join","bandwidth","text","date","Date","sort","a","