t2Refs","_hoisted_12","getEmptyTableMessage","_hoisted_13","userQuestion","rows","placeholder","_hoisted_14","askQuestion","compareAttributes","disabled","selectText2","selectText3","_hoisted_15"],"sources":["D:\\vue_frame\\vue_frame\\wikitable-vue\\client\\src\\components\\Div2.vue"],"sourcesContent":["<template>\r\n\t<div class=\"main-container\">\r\n\t\t<!-- 聊天容器 -->\r\n\t\t<div class=\"chat-container\" @dragover.prevent @drop=\"handleDrop\">\r\n\t\t\t<!-- 历史对话记录 -->\r\n\t\t\t<div class=\"chat-history\">\r\n\t\t\t\t<template v-for=\"(message, index) in chatHistory\" :key=\"index\">\r\n\t\t\t\t\t<div v-if=\"message?.content\" :class=\"['message', message.role]\">\r\n\t\t\t\t\t\t<div class=\"message-content\">\r\n\t\t\t\t\t\t\t<strong>{{ message.role === \"user\" ? \"用户\" : \"GPT\" }}:</strong>\r\n\t\t\t\t\t\t\t<p v-html=\"message.content\"></p>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</template>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\t\t<!-- 视觉内容容器 -->\r\n\t\t<div class=\"vis-container\">\r\n\t\t\t<!-- 三列对比表格 -->\r\n\t\t\t<div class=\"comparison-table\">\r\n\t\t\t\t<!-- 表头 -->\r\n\t\t\t\t<div class=\"table-header\">\r\n\t\t\t\t\t<div class=\"header-cell\">文章1属性可视化</div>\r\n\t\t\t\t\t<div class=\"header-cell\">公共属性</div>\r\n\t\t\t\t\t<div class=\"header-cell\">文章2属性可视化</div>\r\n\t\t\t\t</div>\r\n\r\n\t\t\t\t<!-- 表格内容 -->\r\n\t\t\t\t<div class=\"table-body\">\r\n\t\t\t\t\t<template v-if=\"commonAttributes.length > 0\">\r\n\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\tv-for=\"(attr, index) in commonAttributes\"\r\n\t\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t\tclass=\"table-row\">\r\n\t\t\t\t\t\t\t<!-- 文章1图表 -->\r\n\t\t\t\t\t\t\t<div class=\"table-cell chart-cell\">\r\n\t\t\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\t\t\t:ref=\"\r\n\t\t\t\t\t\t\t\t\t\tel => {\r\n\t\t\t\t\t\t\t\t\t\t\tif (el) chart1Refs[index] = el;\r\n\t\t\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t\t\t\"\r\n\t\t\t\t\t\t\t\t\tclass=\"chart-box\"></div>\r\n\t\t\t\t\t\t\t</div>\r\n\r\n\t\t\t\t\t\t\t<!-- 公共属性名称 -->\r\n\t\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\t\tclass=\"table-cell attr-cell\"\r\n\t\t\t\t\t\t\t\t@click=\"selectAttribute(attr, index)\">\r\n\t\t\t\t\t\t\t\t{{ attr }}\r\n\t\t\t\t\t\t\t</div>\r\n\r\n\t\t\t\t\t\t\t<!-- 文章2图表 -->\r\n\t\t\t\t\t\t\t<div class=\"table-cell chart-cell\">\r\n\t\t\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\t\t\t:ref=\"\r\n\t\t\t\t\t\t\t\t\t\tel => {\r\n\t\t\t\t\t\t\t\t\t\t\tif (el) chart2Refs[index] = el;\r\n\t\t\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t\t\t\"\r\n\t\t\t\t\t\t\t\t\tclass=\"chart-box\"></div>\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</template>\r\n\r\n\t\t\t\t\t<!-- 空状态 -->\r\n\t\t\t\t\t<div v-else class=\"empty-table\">\r\n\t\t\t\t\t\t{{ getEmptyTableMessage() }}\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\r\n\t\t\t<!-- 输入框和操作按钮 -->\r\n\t\t\t<div class=\"input-container\">\r\n\t\t\t\t<textarea\r\n\t\t\t\t\tv-model=\"userQuestion\"\r\n\t\t\t\t\trows=\"2\"\r\n\t\t\t\t\tplaceholder=\"请输入你想问的问题...\"></textarea>\r\n\t\t\t\t<div class=\"button-container\">\r\n\t\t\t\t\t<button @click=\"askQuestion\">发送</button>\r\n\t\t\t\t\t<button\r\n\t\t\t\t\t\t@click=\"compareAttributes\"\r\n\t\t\t\t\t\tclass=\"submit-btn\"\r\n\t\t\t\t\t\t:disabled=\"!selectText2 || !selectText3\">\r\n\t\t\t\t\t\t属性对比可视化\r\n\t\t\t\t\t</button>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script setup>\r\n\timport { ref, onMounted, onUnmounted } from \"vue\";\r\n\timport bus from \"@/js/eventBus.js\";\r\n\timport {\r\n\t\trenderLineChart,\r\n\t\trenderBarChart,\r\n\t\trenderNonVisualChart\r\n\t} from \"@/js/chartUtils\";\r\n\r\n\t// 使用数组存储图表引用\r\n\tconst chart1Refs = ref([]);\r\n\tconst chart2Refs = ref([]);\r\n\r\n\t// 响应式数据\r\n\tconst userQuestion = ref(\"\");\r\n\tconst chatHistory = ref([]);\r\n\tconst selectText2 = ref(\"\");\r\n\tconst selectText3 = ref(\"\");\r\n\tconst article1Data = ref(null);\r\n\tconst article2Data = ref(null);\r\n\tconst commonAttributes = ref([]);\r\n\tconst selectedAttributeIndex = ref(-1);\r\n\r\n\t// 获取空表格提示信息\r\n\tconst getEmptyTableMessage = () => {\r\n\t\tif (!selectText2.value && !selectText3.value) return \"请先选择两篇文章\";\r\n\t\tif (!selectText2.value) return \"请先选择文章1\";\r\n\t\tif (!selectText3.value) return \"请先选择文章2\";\r\n\t\treturn \"无公共属性可对比\";\r\n\t};\r\n\r\n\t// 安全的文本提取\r\n\tconst getPlainTextFromSelection = htmlContent => {\r\n\t\tif (!htmlContent) return \"\";\r\n\t\ttry {\r\n\t\t\tconst container = document.createElement(\"div\");\r\n\t\t\tcontainer.innerHTML = htmlContent;\r\n\t\t\treturn container.textContent || container.innerText || \"\";\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"文本提取错误:\", error);\r\n\t\t\treturn \"\";\r\n\t\t}\r\n\t};\r\n\r\n\t// 事件处理\r\n\tconst handleSelection = (data, source) => {\r\n\t\tif (!data?.content) {\r\n\t\t\tconsole.warn(`无效数据来自 ${source}:`, data);\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\tconst plainText = getPlainTextFromSelection(data.content);\r\n\t\tif (source === \"div1\") {\r\n\t\t\tselectText2.value = plainText;\r\n\t\t} else if (source === \"div3\") {\r\n\t\t\tselectText3.value = plainText;\r\n\t\t}\r\n\t};\r\n\r\n\t// 选择属性\r\n\tconst selectAttribute = (attr, index) => {\r\n\t\tif (!attr || index < 0 || index >= commonAttributes.value.length) {\r\n\t\t\tconsole.warn(\"无效属性选择:\", attr, index);\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\tselectedAttributeIndex.value = index;\r\n\t\trenderCharts(index);\r\n\t};\r\n\r\n\t// 渲染图表\r\n\tconst renderCharts = index => {\r\n\t\tif (index < 0 || !article1Data.value || !article2Data.value) return;\r\n\r\n\t\tconst attr = commonAttributes.value[index];\r\n\t\tif (!attr) return;\r\n\r\n\t\ttry {\r\n\t\t\tconst chart1El = chart1Refs.value[index];\r\n\t\t\tconst chart2El = chart2Refs.value[index];\r\n\r\n\t\t\tif (chart1El && article1Data.value[attr]) {\r\n\t\t\t\trenderChart(chart1El, article1Data.value[attr], \"Line Chart\");\r\n\t\t\t}\r\n\r\n\t\t\tif (chart2El && article2Data.value[attr]) {\r\n\t\t\t\trenderChart(chart2El, article2Data.value[attr], \"Line Chart\");\r\n\t\t\t}\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"图表渲染错误:\", error);\r\n\t\t}\r\n\t};\r\n\r\n\t// 安全渲染图表\r\n\tconst renderChart = (container, data, type) => {\r\n\t\tif (!container || !data) return;\r\n\r\n\t\ttry {\r\n\t\t\t// 先清空容器\r\n\t\t\tcontainer.innerHTML = \"\";\r\n\r\n\t\t\tswitch (type) {\r\n\t\t\t\tcase \"Line Chart\":\r\n\t\t\t\t\trenderLineChart(container, data.data || [], data.options || {});\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"Bar Chart\":\r\n\t\t\t\t\trenderBarChart(container, data);\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tdefault:\r\n\t\t\t\t\trenderNonVisualChart(container, null, {\r\n\t\t\t\t\t\tmessage: \"不支持的图表类型\"\r\n\t\t\t\t\t});\r\n\t\t\t}\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"图表渲染失败:\", error);\r\n\t\t\trenderNonVisualChart(container, null, { message: \"渲染出错\" });\r\n\t\t}\r\n\t};\r\n\r\n\t// 对比属性\r\n\tconst compareAttributes = async () => {\r\n\t\tif (!selectText2.value || !selectText3.value) {\r\n\t\t\talert(\"请先选择两段文本！\");\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\ttry {\r\n\t\t\tconst response = await api.post(\"compare_attributes\", {\r\n\t\t\t\ttext1: selectText2.value,\r\n\t\t\t\ttext2: selectText3.value\r\n\t\t\t});\r\n\r\n\t\t\tif (!response || response.error) {\r\n\t\t\t\tthrow new Error(response?.error || \"请求失败\");\r\n\t\t\t}\r\n\r\n\t\t\tarticle1Data.value = response.article1 || {};\r\n\t\t\tarticle2Data.value = response.article2 || {};\r\n\t\t\tcommonAttributes.value = response.common_attributes || [];\r\n\t\t\tselectedAttributeIndex.value = -1;\r\n\r\n\t\t\t// 重置图表引用数组\r\n\t\t\tchart1Refs.value = [];\r\n\t\t\tchart2Refs.value = [];\r\n\r\n\t\t\tif (commonAttributes.value.length > 0) {\r\n\t\t\t\t// 默认选择第一个属性\r\n\t\t\t\tnextTick(() => {\r\n\t\t\t\t\tselectAttribute(commonAttributes.value[0], 0);\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t} catch (error) {\r\n\t\t\tconsole.error(\"属性对比失败:\", error);\r\n\t\t\tchatHistory.value.push({\r\n\t\t\t\trole: \"assistant\",\r\n\t\t\t\tcontent: `属性对比失败: ${error.message}`\r\n\t\t\t});\r\n