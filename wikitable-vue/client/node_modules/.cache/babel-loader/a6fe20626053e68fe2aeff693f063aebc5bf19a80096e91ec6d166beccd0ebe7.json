isted_12"],"sources":["D:\\vue_frame\\vue_frame\\wikitable-vue\\client\\src\\components\\compoents_base\\FullChart.vue"],"sourcesContent":["<template>\r\n\t<!-- 透明马赛克背景模态框 -->\r\n\t<div\r\n\t\tv-if=\"showFullChartModal\"\r\n\t\tclass=\"full-chart-modal\"\r\n\t\t@click.self=\"closeFullChart\">\r\n\t\t<!-- 动态马赛克背景 -->\r\n\t\t<div class=\"mosaic-bg\"></div>\r\n\r\n\t\t<!-- 图表内容容器 -->\r\n\t\t<div class=\"modal-content\">\r\n\t\t\t<button class=\"close-btn\" @click=\"closeFullChart\">\r\n\t\t\t\t<svg\r\n\t\t\t\t\twidth=\"24\"\r\n\t\t\t\t\theight=\"24\"\r\n\t\t\t\t\tviewBox=\"0 0 24 24\"\r\n\t\t\t\t\tfill=\"none\"\r\n\t\t\t\t\txmlns=\"http://www.w3.org/2000/svg\">\r\n\t\t\t\t\t<path\r\n\t\t\t\t\t\td=\"M18 6L6 18\"\r\n\t\t\t\t\t\tstroke=\"#666\"\r\n\t\t\t\t\t\tstroke-width=\"2\"\r\n\t\t\t\t\t\tstroke-linecap=\"round\" />\r\n\t\t\t\t\t<path\r\n\t\t\t\t\t\td=\"M6 6L18 18\"\r\n\t\t\t\t\t\tstroke=\"#666\"\r\n\t\t\t\t\t\tstroke-width=\"2\"\r\n\t\t\t\t\t\tstroke-linecap=\"round\" />\r\n\t\t\t\t</svg>\r\n\t\t\t</button>\r\n\r\n\t\t\t<h3 class=\"chart-title\">\r\n\t\t\t\t{{ currentChart.title }} - {{ currentChart.field.key }}\r\n\t\t\t</h3>\r\n\r\n\t\t\t<div class=\"chart-container\">\r\n\t\t\t\t<!-- 文本显示 -->\r\n\t\t\t\t<template v-if=\"currentChart.field.visualization === 'text-only'\">\r\n\t\t\t\t\t<div class=\"full-text\">\r\n\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\tv-for=\"(item, index) in formattedData\"\r\n\t\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t\tclass=\"text-item\">\r\n\t\t\t\t\t\t\t{{ formatDisplayValue(item) }}\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</template>\r\n\r\n\t\t\t\t<!-- 饼图 -->\r\n\t\t\t\t<template v-else-if=\"currentChart.field.visualization === 'pie-chart'\">\r\n\t\t\t\t\t<PieChart v-if=\"hasData\" :data=\"pieChartData\" :show-legend=\"true\" />\r\n\t\t\t\t\t<div v-else class=\"no-data\">无可用数据</div>\r\n\t\t\t\t</template>\r\n\r\n\t\t\t\t<!-- 柱状图 -->\r\n\t\t\t\t<template v-else-if=\"currentChart.field.visualization === 'bar-chart'\">\r\n\t\t\t\t\t<BarChart\r\n\t\t\t\t\t\tv-if=\"hasData\"\r\n\t\t\t\t\t\t:data=\"barChartData\"\r\n\t\t\t\t\t\t:field-key=\"currentChart.field.key\"\r\n\t\t\t\t\t\t:show-value=\"true\"\r\n\t\t\t\t\t\t:horizontal=\"barChartData.length > 5\" />\r\n\t\t\t\t\t<div v-else class=\"no-data\">无可用数据</div>\r\n\t\t\t\t</template>\r\n\r\n\t\t\t\t<!-- 折线图 -->\r\n\t\t\t\t<template v-else-if=\"currentChart.field.visualization === 'line-chart'\">\r\n\t\t\t\t\t<LineChart\r\n\t\t\t\t\t\tv-if=\"hasData\"\r\n\t\t\t\t\t\t:data=\"lineChartData\"\r\n\t\t\t\t\t\t:show-trend=\"true\"\r\n\t\t\t\t\t\t:show-points=\"lineChartData.length <= 12\" />\r\n\t\t\t\t\t<div v-else class=\"no-data\">无可用数据</div>\r\n\t\t\t\t</template>\r\n\r\n\t\t\t\t<!-- 默认显示 -->\r\n\t\t\t\t<template v-else>\r\n\t\t\t\t\t<div class=\"full-text\">\r\n\t\t\t\t\t\t<div\r\n\t\t\t\t\t\t\tv-for=\"(item, index) in formattedData\"\r\n\t\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t\tclass=\"text-item\">\r\n\t\t\t\t\t\t\t{{ formatDisplayValue(item) }}\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</template>\r\n\t\t\t</div>\r\n\r\n\t\t\t<div v-if=\"currentChart.field.legend\" class=\"chart-legend\">\r\n\t\t\t\t{{ currentChart.field.legend }}\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script setup>\r\n\timport { computed } from \"vue\";\r\n\timport PieChart from \"./charts/PieChart.vue\";\r\n\timport BarChart from \"./charts/BarChart.vue\";\r\n\timport LineChart from \"./charts/LineChart.vue\";\r\n\r\n\tconst props = defineProps({\r\n\t\tshowFullChartModal: Boolean,\r\n\t\tcurrentChart: Object\r\n\t});\r\n\r\n\tconst emit = defineEmits([\"closeFullChart\"]);\r\n\r\n\t// 数据标准化处理\r\n\tconst normalizedData = computed(() => {\r\n\t\tif (!props.currentChart?.data) return [];\r\n\r\n\t\tconst rawData = props.currentChart.data;\r\n\t\tif (Array.isArray(rawData)) {\r\n\t\t\treturn rawData.map(item => ({\r\n\t\t\t\tvalue: item?.value ?? (typeof item === \"number\" ? item : null),\r\n\t\t\t\traw: item?.raw ?? String(item?.value ?? item),\r\n\t\t\t\tunit: item?.unit ?? null,\r\n\t\t\t\tyear: item?.year ?? null,\r\n\t\t\t\tcurrency: item?.currency ?? null\r\n\t\t\t}));\r\n\t\t}\r\n\r\n\t\treturn [\r\n\t\t\t{\r\n\t\t\t\tvalue: rawData?.value ?? (typeof rawData === \"number\" ? rawData : null),\r\n\t\t\t\traw: rawData?.raw ?? String(rawData?.value ?? rawData),\r\n\t\t\t\tunit: rawData?.unit ?? null,\r\n\t\t\t\tyear: rawData?.year ?? null,\r\n\t\t\t\tcurrency: rawData?.currency ?? null\r\n\t\t\t}\r\n\t\t];\r\n\t});\r\n\r\n\t// 格式化显示值\r\n\tconst formatDisplayValue = item => {\r\n\t\tif (!item) return \"-\";\r\n\r\n\t\t// 优先显示原始文本\r\n\t\tif (item.raw && item.raw !== \"null\" && item.raw !== \"undefined\") {\r\n\t\t\tlet text = item.raw;\r\n\t\t\tif (item.unit) text += ` ${item.unit}`;\r\n\t\t\tif (item.year) text += ` (${item.year})`;\r\n\t\t\treturn text;\r\n\t\t}\r\n\r\n\t\t// 数值格式化\r\n\t\tif (item.value !== null) {\r\n\t\t\tlet num = item.value;\r\n\t\t\tlet text = \"\";\r\n\r\n\t\t\t// 百分比处理\r\n\t\t\tif (props.currentChart.field.type === \"percentage\") {\r\n\t\t\t\treturn `${num > 0 ? \"+\" : \"\"}${num.toFixed(1)}%`;\r\n\t\t\t}\r\n\r\n\t\t\t// 货币格式化\r\n\t\t\tif (item.currency) {\r\n\t\t\t\treturn `${item.currency} ${\r\n\t\t\t\t\tMath.abs(num) >= 1e6\r\n\t\t\t\t\t\t? (num / 1e6).toFixed(2) + \"M\"\r\n\t\t\t\t\t\t: num.toLocaleString()\r\n\t\t\t\t}`;\r\n\t\t\t}\r\n\r\n\t\t\t// 大数值处理\r\n\t\t\tif (Math.abs(num) >= 1e8) {\r\n\t\t\t\ttext = (num / 1e8).toFixed(2) + \"亿\";\r\n\t\t\t} else if (Math.abs(num) >= 1e4) {\r\n\t\t\t\ttext = (num / 1e4).toFixed(1) + \"万\";\r\n\t\t\t} else {\r\n\t\t\t\ttext = num.toLocaleString();\r\n\t\t\t}\r\n\r\n\t\t\tif (item.unit) text += ` ${item.unit}`;\r\n\t\t\treturn text;\r\n\t\t}\r\n\r\n\t\treturn \"-\";\r\n\t};\r\n\r\n\t// 各图表数据适配\r\n\tconst pieChartData = computed(() => {\r\n\t\treturn normalizedData.value\r\n\t\t\t.filter(item => item.value !== null)\r\n\t\t\t.map(item => ({\r\n\t\t\t\tname: item.raw || String(item.value),\r\n\t\t\t\tvalue: item.value,\r\n\t\t\t\