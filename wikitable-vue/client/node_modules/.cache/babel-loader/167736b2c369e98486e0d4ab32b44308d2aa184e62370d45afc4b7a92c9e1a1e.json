{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, onUnmounted, nextTick } from \"vue\";\nimport bus from \"@/js/eventBus.js\";\nimport * as d3 from \"d3\";\nimport { renderLineChart, renderBarChart, renderPieChart, renderNonVisualChart, renderRadarChart, renderStackedBarChart, renderScatterChart } from \"@/js/chartUtils\";\n\n// 响应式数据\n\nexport default {\n  __name: 'Div2',\n  setup(__props, _ref) {\n    let {\n      expose: __expose\n    } = _ref;\n    __expose();\n    const userQuestion = ref(\"\");\n    const chatHistory = ref([]);\n    const selectText2 = ref(null);\n    const selectText3 = ref(null);\n    const article1Data = ref(null);\n    const article2Data = ref(null);\n    const commonAttributes = ref([]);\n    const selectedAttribute = ref(null);\n    const chart1 = ref(null);\n    const chart2 = ref(null);\n\n    // 安全的内容提取函数\n    const getPlainTextFromSelection = htmlContent => {\n      if (!htmlContent) {\n        console.warn(\"htmlContent is null or undefined\");\n        return \"\";\n      }\n      try {\n        const container = document.createElement(\"div\");\n        container.innerHTML = htmlContent;\n        return container.textContent || container.innerText || \"\";\n      } catch (error) {\n        console.error(\"Error extracting text:\", error);\n        return \"\";\n      }\n    };\n\n    // 增强的事件处理函数\n    const handleSelection = (data, source) => {\n      if (!data) {\n        console.error(`Received null data from ${source}`);\n        return;\n      }\n      const content = data?.content;\n      if (!content) {\n        console.error(`Data missing 'content' property from ${source}`, data);\n        return;\n      }\n      const plainText = getPlainTextFromSelection(content);\n      if (!plainText) {\n        console.warn(`Extracted empty text from ${source}`);\n      }\n      if (source === \"div1\") {\n        selectText2.value = plainText;\n      } else if (source === \"div3\") {\n        selectText3.value = plainText;\n      }\n    };\n\n    // 安全的事件处理器\n    const handleDiv1Event = data => {\n      console.log(\"div1_Event received\", data);\n      handleSelection(data, \"div1\");\n    };\n    const handleDiv3Event = data => {\n      console.log(\"div3_Event received\", data);\n      handleSelection(data, \"div3\");\n    };\n\n    // 生命周期钩子\n    onMounted(() => {\n      bus.on(\"div1_Event\", handleDiv1Event);\n      bus.on(\"div3_Event\", handleDiv3Event);\n    });\n    onUnmounted(() => {\n      bus.off(\"div1_Event\", handleDiv1Event);\n      bus.off(\"div3_Event\", handleDiv3Event);\n    });\n\n    // 对比属性\n    const compareAttributes = async () => {\n      if (!selectText2.value || !selectText3.value) {\n        alert(\"请先选择两段文本！\");\n        return;\n      }\n      try {\n        chatHistory.value.push({\n          role: \"user\",\n          content: \"正在对比两篇文章的属性...\"\n        });\n        const response = await api.post(\"compare_attributes\", {\n          text1: selectText2.value,\n          text2: selectText3.value\n        });\n        if (!response) {\n          throw new Error(\"未收到响应\");\n        }\n        if (response.error) {\n          throw new Error(response.error);\n        }\n        article1Data.value = response.article1 || {};\n        article2Data.value = response.article2 || {};\n        commonAttributes.value = response.common_attributes || [];\n        if (commonAttributes.value.length > 0) {\n          selectedAttribute.value = commonAttributes.value[0];\n          nextTick(renderAttributeCharts);\n        }\n        chatHistory.value.push({\n          role: \"assistant\",\n          content: `找到 ${commonAttributes.value.length} 个公共属性`\n        });\n      } catch (error) {\n        console.error(\"属性对比失败:\", error);\n        chatHistory.value.push({\n          role: \"assistant\",\n          content: `属性对比失败: ${error.message}`\n        });\n      }\n    };\n\n    // 渲染属性图表\n    const renderAttributeCharts = () => {\n      if (!selectedAttribute.value || !article1Data.value || !article2Data.value) {\n        console.warn(\"缺少必要的渲染数据\");\n        return;\n      }\n      try {\n        const attr1Data = article1Data.value[selectedAttribute.value];\n        const attr2Data = article2Data.value[selectedAttribute.value];\n        if (!attr1Data || !attr2Data) {\n          throw new Error(`找不到属性 ${selectedAttribute.value} 的数据`);\n        }\n        if (chart1.value) {\n          renderChart(chart1.value, attr1Data, \"Line Chart\");\n        }\n        if (chart2.value) {\n          renderChart(chart2.value, attr2Data, \"Line Chart\");\n        }\n      } catch (error) {\n        console.error(\"渲染图表失败:\", error);\n        chatHistory.value.push({\n          role: \"assistant\",\n          content: `渲染图表失败: ${error.message}`\n        });\n      }\n    };\n\n    // 选择属性\n    const selectAttribute = attr => {\n      if (!attr || !commonAttributes.value.includes(attr)) {\n        console.warn(\"无效的属性选择:\", attr);\n        return;\n      }\n      selectedAttribute.value = attr;\n      nextTick(renderAttributeCharts);\n    };\n\n    // 安全的图表渲染\n    const renderChart = (container, data, chartType) => {\n      if (!container) {\n        console.error(\"图表容器不存在\");\n        return;\n      }\n      try {\n        // 清空容器\n        container.innerHTML = \"\";\n        if (!data || typeof data !== \"object\") {\n          renderNonVisualChart(container, null, {\n            message: \"数据无效\"\n          });\n          return;\n        }\n        switch (chartType) {\n          case \"Line Chart\":\n            renderLineChart(container, data.data || [], data.options || {});\n            break;\n          case \"Bar Chart\":\n            renderBarChart(container, data);\n            break;\n          // 其他图表类型...\n          default:\n            renderNonVisualChart(container, null, {\n              message: `不支持的图表类型: ${chartType}`\n            });\n        }\n      } catch (error) {\n        console.error(\"图表渲染错误:\", error);\n        renderNonVisualChart(container, null, {\n          message: \"图表渲染出错\"\n        });\n      }\n    };\n    const __returned__ = {\n      userQuestion,\n      chatHistory,\n      selectText2,\n      selectText3,\n      article1Data,\n      article2Data,\n      commonAttributes,\n      selectedAttribute,\n      chart1,\n      chart2,\n      getPlainTextFromSelection,\n      handleSelection,\n      handleDiv1Event,\n      handleDiv3Event,\n      compareAttributes,\n      renderAttributeCharts,\n      selectAttribute,\n      renderChart,\n      ref,\n      onMounted,\n      onUnmounted,\n      nextTick,\n      get bus() {\n        return bus;\n      },\n      get d3() {\n        return d3;\n      },\n      get renderLineChart() {\n        return renderLineChart;\n      },\n      get renderBarChart() {\n        return renderBarChart;\n      },\n      get renderPieChart() {\n        return renderPieChart;\n      },\n      get renderNonVisualChart() {\n        return renderNonVisualChart;\n      },\n      get renderRadarChart() {\n        return renderRadarChart;\n      },\n      get renderStackedBarChart() {\n        return renderStackedBarChart;\n      },\n      get renderScatterChart() {\n        return renderScatterChart;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","nextTick","bus","d3","renderLineChart","renderBarChart","renderPieChart","renderNonVisualChart","renderRadarChart","renderStackedBarChart","renderScatterChart","userQuestion","chatHistory","selectText2","selectText3","article1Data","article2Data","commonAttributes","selectedAttribute","chart1","chart2","getPlainTextFromSelection","htmlContent","console","warn","container","document","createElement","innerHTML","textContent","innerText","error","handleSelection","data","source","content","plain