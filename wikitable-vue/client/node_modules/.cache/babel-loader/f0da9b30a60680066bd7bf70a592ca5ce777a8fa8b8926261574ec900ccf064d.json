y => {\r\n\t        return values.map(d => d[key]);\r\n\t    });\r\n\r\n\t    // 设置 X 轴比例尺\r\n\t    const x = d3\r\n\t        .scaleBand()\r\n\t        .domain(cleanedLabels)\r\n\t        .range([0, width])\r\n\t        .padding(0.1);\r\n\r\n\t    // 设置 Y 轴比例尺\r\n\t    const y = d3\r\n\t        .scaleLinear()\r\n\t        .domain([0, d3.max(stackedData.flat())])\r\n\t        .nice()\r\n\t        .range([height, 0]);\r\n\r\n\t    // 绘制堆叠条形图\r\n\t    const stack = d3.stack().keys(keys);\r\n\r\n\t    const layers = stack(values);\r\n\r\n\t    svg\r\n\t        .selectAll(\".layer\")\r\n\t        .data(layers)\r\n\t        .enter()\r\n\t        .append(\"g\")\r\n\t        .attr(\"class\", \"layer\")\r\n\t        .selectAll(\".bar\")\r\n\t        .data(d => d)\r\n\t        .enter()\r\n\t        .append(\"rect\")\r\n\t        .attr(\"x\", (d, i) => x(cleanedLabels[i]))\r\n\t        .attr(\"y\", d => y(d[1]))\r\n\t        .attr(\"width\", x.bandwidth())\r\n\t        .attr(\"height\", d => y(d[0]) - y(d[1]))\r\n\t        .style(\"fill\", (d, i) => options.colors[i % options.colors.length]);\r\n\t}\r\n\t\t\t\tonMounted(() => {\r\n\t\t\t\t\tif (chart.value) {\r\n\t\t\t\t\t\t// 调用渲染函数\r\n\t\t\t\t\t\trenderStackedBarChart(chart.value, json.data, json.options);\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.error(\"chart 容器未找到\");\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\r\n\t\t\t\treturn {\r\n\t\t\t\t\tchart\r\n\t\t\t\t};\r\n\t\t\t}\r\n\t\t};\r\n</script>\r\n\r\n<style>\r\n\t.chart {\r\n\t\twidth: 800px;\r\n\t\theight: 400px;\r\n\t}\r\n</style>\r\n"],"mappings":";gCACCA,mBAAA,CAAc,YAAV,OAAK;;EACJC,GAAG,EAAC,OAAO;EAACC,KAAK,EAAC;;;uBAFxBC,mBAAA,CAAAC,SAAA,SACCC,UAAc,EACdL,mBAAA,CAAqC,OAArCM,UAAqC,8B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}